#include <iostream>
#include<string.h>

using namespace std;

int main() {
  char str[100];
  int i,flag=0;
  cin>>str;
  int length = strlen(str);
  for(i=0;i<length;i++){
    if(str[i]!=str[length-i-1]){
      flag=1;
      break;
    }
  }
  if(flag)
    cout<<"Not a PAlinedrome";
  else
    cout<<"Palendrome";
     
  
  
  
}
