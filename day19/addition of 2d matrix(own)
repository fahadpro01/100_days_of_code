#include <iostream>
using namespace std;
int main() {
  int a[3][3],a1[3][3];
  for(int i=0;i<3;i++)
    {
       for(int j=0;j<3;j++){
             cout<<"Enter the element of 1st set: ";
             cin>>a[i][j];
         }
      }
   for(int i=0;i<3;i++)
    {
       for(int j=0;j<3;j++){
             cout<<"Enter the element of 2nd set : ";
             cin>>a1[i][j];
         }
      }
     for(int i=0;i<3;i++)
    {
       for(int j=0;j<3;j++){
             cout<<" "<< a[i ] [j ] + a1[i][j]<<" ";
         }
      cout<<"\n";
      
      }
     
  
  }
