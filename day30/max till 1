#include <iostream>
using namespace std;

int main() {
   int mx=-1999999;//if we use the -199999 then we can generate all min value , otherwise , it will be 555555.
  int n;
  cin>>n;
  int a[n];
  for(int i=0;i<n;i++){
    cin>>a[i];
  }
   for(int i=0;i<n;i++){
      mx = max(mx,a[i]);
      cout<<mx<<endl;
  }
  
}
