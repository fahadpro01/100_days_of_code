#include <iostream>
using namespace std;

int main() {
   
  int n;
  cin>>n;
  int a[n];
  for(int i=0;i<n;i++){
    cin>>a[i];
  }
    int cursum=0;
   for(int i=0;i<n;i++){
      cursum=0;
     for(int j=i;j<n;j++){
       cursum+=a[j];
       cout<<"SUB ARRAYS ARE: "<<cursum<<endl;
     }
     
  }
  
}
