void scores(long long a[], long long b[], int &ca, int &cb)
    {
         int a_count=0,b_count=0,i;
        // Your code goes here  
        for(int i=0;i<3;i++){
         if(a[i]>b[i]){
             a_count++;
         }
         else
         if(a[i]<b[i]){
             b_count++;
         }
        else{
        continue;
        }
    }
        ca=a_count;
        cb=b_count;
        
    }
