#include <iostream>
using namespace std;

int main() {
  int n;
  cout<<"Enter the number : ";
  cin>>n;
  int sum=0;
  for(int i=1;i<n;i++){
       sum+=i;
  
    if(sum==n){
      cout<<"Triangular Number"<<endl;
      break;
      
      }
    }
     if(sum !=n){
  cout<<"Not a Traingular Number";
    }
    
  
  }
  
