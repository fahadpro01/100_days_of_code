string funnyString(string s) {
    string rev=s;
    int temp1,temp2;
    reverse( rev.begin(), rev.end());
    int n= s.size();
    for(int i=0;i<n-1;i++){
         temp1=  abs((s[i]-s[i+1]));
        temp2= abs((rev[i]-rev[i+1]));
        
         if(temp1!= temp2){
              return "Not Funny"; 
           
         }
            
    }
            return "Funny";
          
    

}
